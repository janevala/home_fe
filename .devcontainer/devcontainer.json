{
	"name": "Flutter Dev Container",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"runArgs": [
		"--privileged",
		"--network=host"
	],
	"forwardPorts": [
		7070
	],
	"portsAttributes": {
		"7070": {
			"label": "Application",
			"onAutoForward": "notify"
		}
	},
	"postCreateCommand": "flutter pub get",
	"remoteUser": "root"
}
